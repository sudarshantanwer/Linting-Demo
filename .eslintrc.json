{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["error", 2],
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-var": "error",
    "no-unused-vars": "error",
    "space-before-function-paren": ["error", "always"],
    "arrow-spacing": "error",
    "no-console": "warn",
    "comma-dangle": ["error", "always-multiline"]
    }
}
